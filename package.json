{
	"name": "frequently-used-files",
	"displayName": "frequently-used-files",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:frequentlyUsedFiles"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "frequentlyUsedFiles-explorer",
					"title": "Frequent Files AB",
					"icon": "media/dep.svg"
				}
			]
		},
		"views": {
			"frequentlyUsedFiles-explorer": [
				{
					"id": "frequentlyUsedFiles",
					"name": "Frequently Files PE",
					"icon": "media/dep.svg",
					"contextualTitle": "Frequent Files PE"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.63.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4",
		"@vscode/test-electron": "^1.6.2"
	}
}